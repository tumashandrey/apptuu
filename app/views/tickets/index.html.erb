<p id="notice"><%= notice %></p>

<h1>Список билетов</h1>
<h2><%= link_to 'На главную',  welcome_index_path %></h2>
<table>
  <thead>
  <tr>
    <th>Номер</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @tickets.each do |ticket| %>
      <tr>
        <%= render ticket %>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Добавить билет', new_ticket_path %>
